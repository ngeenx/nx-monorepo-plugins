{
  "$schema": "https://json-schema.org/schema",
  "$id": "ChromeExtensionGenerator",
  "title": "",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Name of the library.",
      "pattern": "^[a-z0-9-]+$",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What project name would you like to use?"
    }
  },
  "required": ["name"]
}
